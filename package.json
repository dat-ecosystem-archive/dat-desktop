{
  "name": "dat-desktop",
  "productName": "Dat Desktop",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "juliangruber/dat-desktop",
  "description": "Dat Desktop App",
  "dependencies": {
    "bytes": "^2.4.0",
    "bytewise": "^1.1.0",
    "dat-button": "^2.2.0",
    "dat-design": "^1.1.4",
    "dat-encoding": "^2.0.1",
    "dat-header": "^1.0.3",
    "electron-prebuilt": "1.1.3",
    "hyperdrive": "^6.5.1",
    "hyperdrive-archive-swarm": "^3.1.0",
    "hyperdrive-import-files": "^1.1.0",
    "hyperdrive-ui": "karissa/hyperdrive-ui#alternative-api",
    "level": "^1.4.0",
    "level-live-stream": "^1.4.11",
    "minimist": "^1.2.0",
    "random-access-file": "^1.2.0",
    "rimraf": "^2.5.2",
    "yo-yo": "^1.2.1"
  },
  "scripts": {
    "start": "electron .",
    "test": "semistandard",
    "package": "rm -Rf dist/*; electron-packager . --platform=darwin --arch=x64 --out=dist/ --extend-info extend.plist --app-bundle-id=com.dat.desktop --icon=public/img/app.icns && appdmg dmg.json dist/Dat.dmg",
    "rebuild": "npm rebuild --runtime=electron --target=1.1.3 --disturl=https://atom.io/download/atom-shell --abi=48",
    "build-css": "node-sass --include-path scss scss/main.scss public/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "devDependencies": {
    "appdmg": "^0.4.5",
    "electron-packager": "^7.0.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "semistandard": "^8.0.0"
  }
}
