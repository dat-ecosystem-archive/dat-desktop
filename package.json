{
  "name": "dat-desktop",
  "productName": "Dat",
  "version": "1.0.1",
  "license": "MIT",
  "repository": "datproject/dat-desktop",
  "description": "Dat Desktop App",
  "author": "Dat Team",
  "browserify": {
    "transform": [
      [
        "sheetify/transform",
        {
          "use": "sheetify-nested"
        }
      ]
    ]
  },
  "aliasify": {
    "aliases": {
      "bindings": "bindings-browserify"
    }
  },
  "dependencies": {
    "aliasify": "^2.1.0",
    "bindings-browserify": "^2.0.0",
    "bytewise": "^1.1.0",
    "cache-element": "^2.0.0",
    "choo": "^4.0.0-6",
    "choo-log": "^3.0.0",
    "choo-model": "^1.0.0",
    "choo-persist": "^2.0.0",
    "collect-stream": "^1.1.1",
    "dat-colors": "^3.0.0",
    "dat-doctor": "^1.2.1",
    "dat-encoding": "^4.0.1",
    "dat-icons": "^1.3.0",
    "dat-node": "^0.1.2",
    "electron-default-menu": "^1.0.0",
    "electron-window": "^0.8.1",
    "envobj": "^1.0.2",
    "explain-error": "^1.0.3",
    "global": "^4.3.1",
    "hyperdrive-stats": "^2.2.5",
    "level": "^1.4.0",
    "minimist": "^1.2.0",
    "on-load": "^3.2.0",
    "open": "0.0.5",
    "prettier-bytes": "^1.0.3",
    "sheetify": "^6.0.0",
    "subleveldown": "^2.1.0",
    "tachyons": "^4.5.4",
    "xtend": "^4.0.1",
    "yo-css": "^1.1.0",
    "yo-yo": "^1.3.1"
  },
  "scripts": {
    "bundle": "browserify --fast --ignore-missing --no-builtins --insert-global-vars=\"__filename,__dirname\" --no-browser-field --exclude=electron -g bindings-browserify/transform -g aliasify app.js > bundle.js",
    "watch": "watchify --fast --ignore-missing --no-builtins --insert-global-vars=\"__filename,__dirname\" --no-browser-field --exclude=electron -g bindings-browserify/transform -g aliasify app.js --verbose -o bundle.js",
    "deps": "dependency-check package.json app.js",
    "rebuild": "./scripts/build rebuild",
    "clean": "rm -rf node_modules/",
    "build-background": "./scripts/build background",
    "start": "NODE_ENV=development electron .",
    "test": "standard && npm run deps",
    "postinstall": "./scripts/build postinstall",
    "pack": "build --dir",
    "dist": "build"
  },
  "devDependencies": {
    "browserify": "juliangruber/node-browserify#fix/dat-desktop",
    "bulkify": "^1.4.2",
    "dependency-check": "^2.6.0",
    "electron": "1.4.6",
    "electron-builder": "^8.6.0",
    "nodemon": "^1.9.2",
    "sheetify-nested": "^1.0.2",
    "standard": "^7.1.2",
    "through2": "^2.0.3",
    "watchify": "^3.7.0"
  },
  "build": {
    "appId": "com.datproject.dat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 220,
          "y": 200
        },
        {
          "x": 448,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "protocols": [
      {
        "name": "Dat Link",
        "schemes": [
          "dat"
        ]
      }
    ]
  }
}
