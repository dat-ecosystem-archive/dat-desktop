{
  "name": "dat-desktop",
  "productName": "Dat",
  "version": "2.0.1",
  "license": "MIT",
  "repository": "datproject/dat-desktop",
  "description": "Dat Desktop App",
  "author": "Dat Team",
  "scripts": {
    "start": "electron .",
    "test": "standard",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron": "^1.7.10",
    "standard": "^10.0.3",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "dat-colors": "^3.5.1",
    "dat-encoding": "^5.0.1",
    "dat-icons": "^2.5.1",
    "dat-node": "^3.5.6",
    "prettier-bytes": "^1.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.4.0",
    "tachyons": "^4.9.1"
  },
  "build": {
    "appId": "com.datproject.dat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 220,
          "y": 200
        },
        {
          "x": 448,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "protocols": [
      {
        "name": "Dat Link",
        "schemes": [
          "dat"
        ]
      }
    ]
  }
}
