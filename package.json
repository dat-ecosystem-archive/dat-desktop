{
  "name": "dat-desktop",
  "productName": "Dat",
  "version": "2.0.1",
  "license": "MIT",
  "repository": "datproject/dat-desktop",
  "description": "Dat Desktop App",
  "author": "Dat Team",
  "scripts": {
    "start": "electron .",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test babel-tape-runner -r ./unit-tests/_helpers/*.js unit-tests/*.js",
    "lint": "prettier-standard 'app/**/*.js' 'tests/**/*.js' 'lib/**/*.js' 'unit-tests/*.js' && standard",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "electron": "^1.7.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-adapter-react-helper": "^1.2.3",
    "prettier-standard": "^8.0.0",
    "standard": "^10.0.3",
    "tape": "^4.9.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "dat-colors": "^3.5.1",
    "dat-doctor": "^1.3.1",
    "dat-encoding": "^5.0.1",
    "dat-icons": "^2.5.1",
    "dat-node": "^3.5.7",
    "electron-default-menu": "^1.0.1",
    "minimist": "^1.2.0",
    "mirror-folder": "^2.1.1",
    "mkdirp-promise": "^5.0.1",
    "ms": "^2.1.1",
    "polished": "^1.9.2",
    "prettier-bytes": "^1.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-file-drop": "^0.1.9",
    "react-redux": "^5.0.6",
    "react-swap": "^2.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.4.0",
    "tachyons": "^4.9.1",
    "util-promisify": "^2.1.0"
  },
  "build": {
    "appId": "com.datproject.dat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 220,
          "y": 200
        },
        {
          "x": 448,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "protocols": [
      {
        "name": "Dat Link",
        "schemes": [
          "dat"
        ]
      }
    ]
  }
}
