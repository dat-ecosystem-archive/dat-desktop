{
  "name": "dat-desktop",
  "productName": "Dat",
  "version": "1.0.2",
  "license": "MIT",
  "repository": "datproject/dat-desktop",
  "description": "Dat Desktop App",
  "author": "Dat Team",
  "dependencies": {
    "base-elements": "^2.2.1",
    "choo": "^4.0.0-6",
    "choo-log": "^3.0.0",
    "choo-persist": "^2.0.0",
    "dat-colors": "^3.3.0",
    "dat-doctor": "^1.2.1",
    "dat-encoding": "^4.0.1",
    "dat-icons": "^1.3.0",
    "electron-auto-updater": "^0.9.2",
    "electron-default-menu": "^1.0.0",
    "electron-window": "^0.8.1",
    "envobj": "^1.0.2",
    "explain-error": "^1.0.3",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ms": "^0.7.2",
    "multidat": "^2.0.0",
    "nanomorph": "^2.1.3",
    "open": "0.0.5",
    "prettier-bytes": "^1.0.3",
    "rimraf": "^2.5.4",
    "run-waterfall": "^1.1.3",
    "sheetify": "^6.0.0",
    "tachyons": "^4.5.4",
    "toiletdb": "^1.2.0",
    "xhr": "^2.3.3",
    "xtend": "^4.0.1",
    "yo-css": "^1.1.0"
  },
  "scripts": {
    "bundle": "./scripts/build bundle",
    "watch": "scripts/browserify.js watch",
    "deps": "dependency-check . --entry ./app.js && dependency-check . --entry ./app.js --extra -i tachyons -i dat-colors --no-dev",
    "rebuild": "./scripts/build rebuild",
    "burnthemall": "rm -rf ~/.electron && npm run clean && npm install && npm run bundle",
    "clean": "rm -rf node_modules/",
    "build-background": "./scripts/build background",
    "start": "NODE_ENV=development electron .",
    "test": "standard && npm run deps",
    "postinstall": "./scripts/build postinstall",
    "pack": "build --dir",
    "dist": "build --publish=onTagOrDraft"
  },
  "devDependencies": {
    "browserify": "^13.3.0",
    "bulkify": "^1.4.2",
    "concat-stream": "^1.6.0",
    "dependency-check": "^2.6.0",
    "electron": "1.4.14",
    "electron-builder": "^8.6.0",
    "envify": "^4.0.0",
    "nodemon": "^1.9.2",
    "sheetify-nested": "^1.0.2",
    "standard": "^7.1.2",
    "through2": "^2.0.3",
    "watchify": "^3.7.0"
  },
  "build": {
    "appId": "com.datproject.dat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 220,
          "y": 200
        },
        {
          "x": 448,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "protocols": [
      {
        "name": "Dat Link",
        "schemes": [
          "dat"
        ]
      }
    ]
  }
}
